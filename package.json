{
    "name": "voby-mobile-datepicker",
    "version": "1.0.0",
    "types": "dist",
    "description": "voby based date picker like native",
    "module": "dist/index.js",
    "main": "dist/index.js",
    "files": [
        "dist/*",
        "dist/**/*"
    ],
    "amdName": "vobyMobileDatePickerTs",
    "repository": "wongchichong/voby-mobile-datepicker",
    "homepage": "https://github.com/wongchichong/voby-mobile-datepicker#readme",
    "keywords": [
        "date",
        "datepicker",
        "mobile",
        "calendar"
    ],
    "scripts": {
        "clean": "rm -rf dist/",
        "build": "vite build",
        "scss": "scss",
        "start": "vite --force --mode dev --host --config vite.config.web.ts",
        "build1": "npm run clean && npm run transpile && npm run size",
        "transpile//": "webpack --config ./webpack.build.config.js",
        "size": "size=$(gzip-size $npm_package_main) && echo \"gzip size: $size\""
    },
    "author": "rainie",
    "devDependencies": {
        "vite": "^4.3.3",
        "vite-plugin-dts": "^2.3.0",
        "vite-plugin-sass-dts": "^1.3.4",
        "vite-plugin-static-copy": "^0.13.1",
        "scss": "^0.2.4"
    },
    "dependencies": {
        "voby": "workspace:../woby"
    },
    "license": "MIT"
}